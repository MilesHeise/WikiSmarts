<div>
  <% User.all.each do |u| %>
    <small><%= u.email %> | user id #: <%= u.id %></small>
    <br>
  <% end %>
</div>

<div>
  <h1>oh look hey form</h1>
  <%= form_for(@collab, url: collaborations_path) do |f| %>
    <%= f.label :user_id %>
    <%= f.text_field :user_id, class: 'form-control', placeholder: "Enter user id of new collaborator" %>
    <%= f.hidden_field :wiki_id, value: @wiki %>
    <%= f.submit "Save", class: 'btn btn-success' %>
  <% end %>
</div>

